:"72"},{"filePath":"136","messages":"137","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"138","messages":"139","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"72"},{"filePath":"140","messages":"141","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/neriram/Desktop/react-projects/react-movie-madness/src/index.js",[],["142","143"],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/App.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/reportWebVitals.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/GlobalStyle.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Header/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Header/Header.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Home.js",["144","145","146"],"import React, { useState, useEffect } from 'react';\n//API \nimport Api from '../Api'\n\n//Imports from config file\nimport { POSTER_SIZE, BACKDROP_SIZE, IMAGE_BASE_URL } from '../config'\n\n//Components\nimport HeroImage from './HeroImage';\nimport Grid from './Grid';\nimport Thumb from './Thumb';\nimport Spinner from './Spinner';\nimport SearchBar from './SearchBar';\nimport Button from './Button';\n//Hook\nimport { useHomeFetch } from '../hooks/useHomeFetch'\n//Image\nimport NoImage from '../images/no_image.jpg';\n\n\nconst Home = () => {\n  const { state, loading, error, searchTerm, setSearchTerm, setIsLoadingMore } = useHomeFetch();\n\n  if (error) return <div>Something went wrong </div>\n\n  return (\n    <>\n      {!searchTerm && state.results[0] ? (\n        <HeroImage\n          image={`${IMAGE_BASE_URL}${BACKDROP_SIZE}${state.results[0].backdrop_path}`}\n          title={state.results[0].original_title}\n          text={state.results[0].overview}\n        />\n      ) : null}\n      <SearchBar setSearchTerm={setSearchTerm} />\n      <Grid header={searchTerm ? 'Search Results' : 'Popular Movies'}>\n          {state.results.map(movie => (\n             <Thumb\n                key={movie.id}\n                clickable\n                image={\n                    movie.poster_path\n                        ? IMAGE_BASE_URL + POSTER_SIZE + movie.poster_path\n                        : NoImage\n                }\n                movideId={movie.id}\n            />\n          ))}\n      </Grid>\n      {loading && <Spinner />}\n      {state.page < state.total_pages && !loading && (\n          <Button \n            text='Load more movies' \n            callback={() => setIsLoadingMore(true)} \n        />\n      )}\n    </>\n  );\n};\n\nexport default Home;","/Users/neriram/Desktop/react-projects/react-movie-madness/src/config.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/Api.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/hooks/useHomeFetch.js",["147"],"import {useEffect, useState, useRef } from 'react';\nimport Api from '../Api'\n\nconst initialState = {\n    page: 0,\n    results: [],\n    total_pages: 0,\n    total_results: 0\n};\n\nexport const useHomeFetch = () => {\n    const [searchTerm, setSearchTerm] = useState('');\n    const [state, setState] = useState(initialState);\n    const [loading, setLoading] = useState(false);\n    const [error, setError] = useState(false);\n    const [isLoadingMore, setIsLoadingMore] = useState(false);\n\n    const fetchMovies = async (page, searchTerm =\"\") => {\n        try {\n            setError(false);\n            setLoading(true);\n\n            const movies = await Api.fetchMovies(searchTerm, page);\n\n            setState(prev => ({\n                ...movies,\n                results:\n                    page > 1 ? [...prev.results, ...movies.results] : [...movies.results]\n            }))\n\n        } catch (error) {\n            setError(true);\n        }\n        setLoading(false);\n    };\n\n    //Initial Render and Search\n    useEffect(() => {\n        setState(initialState);\n        fetchMovies(1, searchTerm)\n    }, [searchTerm])\n\n    //Load More Movies\n    useEffect(() => {\n        if (!isLoadingMore) return;\n        fetchMovies(state.page + 1, searchTerm);\n        setIsLoadingMore(false);\n    }, [isLoadingMore, searchTerm, state.page]);\n\n    return { state, loading, error, searchTerm, setSearchTerm, setIsLoadingMore };\n\n}","/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/HeroImage/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/HeroImage/HeroImage.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Thumb/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Grid/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Thumb/Thumb.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Grid/Grid.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Spinner/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Spinner/Spinner.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/SearchBar/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/SearchBar/SearchBar.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Button/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Button/Button.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/NotFound.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Movie.js",["148"],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/hooks/useMovieFetch.js",["149"],"import React, { useState, useEffect } from 'react';\nimport Api from '../Api';\n\nexport const useMovieFetch = movieId => {\n    const [state, setState] = useState({});\n    const [loading, setLoading] = useState(true);\n    const [error, setError] = useState(false);\n\n    useEffect(() => {\n        const fetchMovie = async() => {\n            try {\n                setLoading(true);\n                setError(false);\n\n                const movie = await Api.fetchMovie(movieId);\n                const credits = await Api.fetchCredits(movieId);\n\n                //Get director to display\n                const directors = credits.crew.filter(\n                    member => member.job === 'Director'\n                );\n                setState({\n                    ...movie,\n                    actors: credits.cast,\n                    directors\n                })\n                setLoading(false);\n            } catch (error) {\n                setError(true);\n            }\n        }\n        fetchMovie();\n    }, [movieId])\n    return { state, loading, error }\n};","/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/BreadCrumb/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/BreadCrumb/BreadCrumb.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/MovieInfo/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/MovieInfo/MovieInfo.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/MovieInfoBar/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/MovieInfoBar/MovieInfoBar.styles.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/helperFunctions.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Actor/index.js",[],"/Users/neriram/Desktop/react-projects/react-movie-madness/src/components/Actor/Actor.styles.js",[],{"ruleId":"150","replace